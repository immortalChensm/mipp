/* pages/我的关注/myattention.wxss */
.edu-att-right-ae{
  display: inline-block;
  width: 51%;
}
.edu-att-l-w{
  width: 70%;
}
.edu-att-l-q{
  width: 25%;
  text-align: center;
}
.edu-att-le{
  width: 40%;
  display: inline-block;
}
.edu-att-topspace-a{
  margin-top: 96rpx;
}
.edu-att-position-w{
  position: fixed;
  top:130rpx;
  width: 100%;
  background-color: #ffffff;
  z-index: 999;
}
.content {
 width: 100%;
 padding: 30rpx; 
 margin-right:0rpx;
 -webkit-transition: all 0.2s;
 transition: all 0.2s;
 -webkit-transform: translateX(180rpx);
 transform: translateX(180rpx);
 margin-left: -180rpx;
}
.del {
  width:180rpx;
  font-size:28rpx;
  color:rgb(255,255,255);
  background:rgb(255,59,48);
  vertical-align:top;
  background-color: orangered;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  -webkit-transform: translateX(180rpx);
  transform: translateX(180rpx);
  -webkit-transition: all 0.2s;
  transition: all 0.2s;
}
 .touch-move-active .content,
.touch-move-active .del {
 -webkit-transform: translateX(0);
 transform: translateX(0);
}
.edu-tea-name-a{
  width: 40%;
}